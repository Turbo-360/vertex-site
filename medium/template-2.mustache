                            <!--end of col-->
                            <div class="col-6 col-lg-3">
                                <h6>Community</h6>
                                <ul class="list-unstyled">
                                    <li>
                                        <a href="#">Forum</a>
                                    </li>
                                    <li>
                                        <a href="#">Knowledgebase</a>
