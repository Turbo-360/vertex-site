                                            <li>
                                                Downloadable Videos
                                            </li>
                                            <li>
                                                Downloadable Code Samples
                                            </li>
                                            <li>
                                                5 Project Maximum
                                            </li>
                                        </ul>
                                        <a href="#" class="btn btn-info mt-0 w-100 mb-4" data-toggle="modal" data-target="#register-modal">JOIN NOW</a>
                                    </div>
                                </div>
                                <!--end card-->
                            </div>
                            <!--end of col-->
                            <div class="col-md-6 col-lg-4">
                                <div class="card pricing card-lg">
                                    <div class="card-body">
                                        <span class="display-3 mt-0 mb-2">$50</span>
                                        <span>12 MONTHS</span>
                                        <hr>
                                        <span class="h6">Includes:</span>
                                        <ul class="list-unstyled" style="height:160px">
                                            <li>
                                                <em>FULL</em> Tutorial Access
                                            </li>
                                            <li>
                                                Downloadable Videos
                                            </li>
                                            <li>
                                                Downloadable Code Samples
                                            </li>
                                            <li>
                                                <em>Unlimited</em> Projects
                                            </li>
                                            <li>
                                                Vertex 360 Developer License
                                            </li>
                                        </ul>
                                        <a id="register-btn-upgrade" href="#" class="btn btn-success mt-0 w-100 mb-4" data-toggle="modal" data-target="#upgrade-modal">UPGRADE</a>
                                        <a id="register-btn-premium" href="#" class="btn btn-success mt-0 w-100 mb-4" data-toggle="modal" data-target="#register-premium-modal" style="display: none;">JOIN NOW</a>
                                    </div>
                                </div>
                                <!--end card-->
                            </div>
                            <!--end of col-->

                        </div>
                        <!--end of row-->
                    </div>

                </div>
                <!--end of tab content-->

            </div>
            <!--end of container-->
        </section>

        <footer class="bg-gray text-light footer-long">
            <div class="container">
                <div class="row">
                    <div class="col-12 col-md-3">
                        <img alt="Image" src="/wingman/img/logo-white.svg" class="mb-4" />
                        <p class="text-muted">
                            &copy; 2018 Medium Rare
